<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="dotnet" arguments=".\EchoIISProxy.dll" 
                  stdoutLogEnabled="true" stdoutLogFile="C:\inSwitch\Logs\EchoIISProxy\IIS" 
                  hostingModel="OutOfProcess" />
    </system.webServer>
  </location>
</configuration>